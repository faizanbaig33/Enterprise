    "npm-run-all": "~4.1.5",
    "postcss": "^8.4.18",
    "prettier": "^2.1.2",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "storybook-addon-next": "^1.7.1",
    "tailwindcss": "^3.2.1",
    "tailwindcss-themer": "^2.0.2",
    "ts-node": "^9.0.0",
    "typescript": "~4.3.5",
    "webpack": "^5.75.0",
    "yaml-loader": "^0.6.0"
  },
  "scripts": {
    "jss": "jss",
    "lint": "eslint ./src/**/*.tsx ./src/**/*.ts ./scripts/**/*.ts",
    "bootstrap": "ts-node --project tsconfig.scripts.json scripts/bootstrap.ts",
    "build": "npm-run-all --serial leprechaun bootstrap next:build",
    "graphql:update": "ts-node --project tsconfig.scripts.json ./scripts/fetch-graphql-introspection-data.ts",
    "next:build": "next build",
    "next:dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "next:start": "next start",
    "scaffold": "ts-node --project tsconfig.scripts.json scripts/scaffold.ts",
    "scaffold:component": "npm run scaffold -- component",
    "start:aw": "npm-run-all --serial bootstrap --parallel next:dev start:watch-components start:watch-fields",
    "start:rba": "npm-run-all --serial bootstrap --parallel next:dev start:watch-components start:watch-fields",
    "start:connected": "npm-run-all --serial leprechaun bootstrap --parallel next:dev start:watch-components start:watch-fields",
    "start:production": "npm-run-all --serial leprechaun bootstrap next:build next:start",
    "start:watch-components": "ts-node --project tsconfig.scripts.json scripts/generate-component-factory.ts --watch",
    "start:watch-fields": "ts-node --project tsconfig.scripts.json scripts/generate-form-factory.ts --watch",
    "storybook": "start-storybook -p 3001",
    "lint:staged": "lint-staged",
    "precommit": "npm-run-all --serial lint:staged leprechaun bootstrap test:silent && npx tsc --project tsconfig.json",
    "test": "jest",
    "test:silent": "jest --silent",
    "leprechaun": "dotnet leprechaun -g -c ../leprechaun.config"
  }
}
